<div class="booyah-box col-xs-10 col-xs-offset-1">
  <h1>Enter Your Bucketlist Details</h1>
  <br />
    <%= simple_form_for @bucketlist do |f| %>
    <%= f.input :name %>
    <br />
    <%= f.input :description %>
    <br />
    <%= f.input :location %>
    <br />
    <%= f.submit 'Create', :class => 'btn btn-primary' %>
  <% end %>
</div>